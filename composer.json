{
    "name": "rongu/sms",
    "description": "Send SMS with Maldives service providers plus online providers such as vonage and twillo",
    "keywords": [
        "rongu",
        "sms"
    ],
    "homepage": "https://github.com/rongu/sms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohd",
            "email": "hello@rongu.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "*",
        "vonage/client": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rongu\\Sms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rongu\\Sms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rongu\\Sms\\SmsServiceProvider"
            ]
        }
    },
    "repositories": {
        "gitlab.com/15193552": {
            "type": "composer",
            "url": "https://gitlab.com/api/v4/group/15193552/-/packages/composer/packages.json"
        }
    }
}
